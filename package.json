{
  "name": "@vcwebnetworks/typescript-with-jest",
  "version": "1.0.0",
  "description": "Initial framework for automated typescript and test based projects",
  "main": "./dist/server/index.js",
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=local tsx watch './src/server/index.ts'",
    "dev:docker": "cross-env NODE_ENV=local docker-compose -f docker-compose.yml up --build",
    "prestart": "npx sequelize-cli db:migrate",
    "start": "node ./dist/server/index.js",
    "updatelibs": "npx npm-check -s -u",
    "check-ts": "npm run lint:eslint && npx tsc -p ./tsconfig.json --noEmit",
    "clean": "rm -rf ./dist",
    "lint:eslint": "eslint --ext ts ./src ./database --fix",
    "lint:prettier": "npx prettier --write ./src",
    "build": "npm run clean && npx tsc -p tsconfig-build.json",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand --no-cache",
    "test:watch": "npm run test -- --watch",
    "test:unit": "npm run test:watch -c ./jest-unit.config.ts",
    "test:integration": "npm run test:watch -c ./jest-integration.config.ts",
    "test:staged": "npm run test -- -o",
    "test:coverage": "npm run test -- --coverage",
    "db:check-ts": "npx tsc ./database/migrations/*.ts --noEmit",
    "db:compile": "./scripts/sequelize-database-compile.sh",
    "db:migrate": "./scripts/sequelize-run-migration.sh",
    "prepare": "husky install"
  },
  "keywords": [
    "nodejs",
    "jest",
    "typescript",
    "ddd",
    "clean-architecture",
    "javascript",
    "structure",
    "express"
  ],
  "author": "Vagner Cardoso <vagner.cardoso@vcwebnetworks.com.br>",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.61",
    "@swc/jest": "0.2.26",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/dotenv": "8.2.0",
    "@types/dottie": "2.0.5",
    "@types/express": "4.17.17",
    "@types/helmet": "4.0.0",
    "@types/jest": "29.5.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/module-alias": "2.0.1",
    "@types/morgan": "1.9.4",
    "@types/node": "20.2.5",
    "@types/response-time": "2.3.5",
    "@types/sequelize": "4.28.15",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/validator": "13.7.17",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "sequelize-cli": "6.6.0",
    "supertest": "6.3.3",
    "typescript": "<=5.1.0"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "dotenv": "16.1.3",
    "dottie": "2.0.3",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "helmet": "7.0.0",
    "http-graceful-shutdown": "3.1.13",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.0",
    "module-alias": "2.2.2",
    "moment-timezone": "0.5.43",
    "morgan": "1.10.0",
    "pg": "8.11.0",
    "pg-hstore": "2.3.4",
    "reflect-metadata": "0.1.13",
    "response-time": "2.3.2",
    "sequelize": "6.32.0",
    "sequelize-typescript": "2.1.5",
    "swagger-ui-express": "4.6.3",
    "tsx": "3.12.7",
    "winston": "3.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
